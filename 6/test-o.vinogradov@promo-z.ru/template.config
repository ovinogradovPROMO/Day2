  <LogoPicture Path="images/logo.png"></LogoPicture>
  <Carousel>
    <Slide Path="http://cs71.advantshop.net/4312.jpg"></Slide>
    <Slide Path="http://cs71.advantshop.net/4313.jpg"></Slide>
    <Slide Path="http://cs71.advantshop.net/4421.jpg"></Slide>
    <Slide Path="http://cs71.advantshop.net/4425.jpg"></Slide>
    <Slide Path="http://cs71.advantshop.net/4426.jpg"></Slide>
    <Slide Path="http://cs71.advantshop.net/4427.jpg"></Slide>
  </Carousel>
  
</Settings>